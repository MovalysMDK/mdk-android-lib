CREATE TABLE IF NOT EXISTS T_MOBJECTTOSYNCHRONIZE(
	ID INTEGER NOT NULL,
	OBJECTID INTEGER NOT NULL,
	OBJECTNAME TEXT NOT NULL,
	CONSTRAINT PK_MOBJECTTOSYNCHRONIZE PRIMARY KEY (ID)
);
		
CREATE TABLE IF NOT EXISTS T_MPARAMETERS(
	ID INTEGER NOT NULL,
	NAME TEXT NOT NULL,
	VALUE TEXT NOT NULL,
	CONSTRAINT PK_MPARAMETERS PRIMARY KEY (ID)
);