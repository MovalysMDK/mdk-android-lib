<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory android:title="@string/preference_category_identification">
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="login"
			android:title="@string/preference_login_title" 
			android:summary="@string/preference_login_summary"
			android:dialogTitle="@string/preference_login_dialogTitle"
			android:inputType="textUri"/>
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEncryptedPasswordTextPreference android:key="password" 
			android:inputType="textPassword"
			android:title="@string/preference_password_title" 
			android:summary="@string/preference_password_summary"
			android:dialogTitle="@string/preference_password_dialogTitle" />
	</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory>
	<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory android:title="@string/preference_category_server">
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="urlserver"
			android:title="@string/preference_url_title" 
			android:inputType="textUri"
			android:summary="@string/preference_url_summary"
			android:dialogTitle="@string/preference_url_dialogTitle"
			android:defaultValue="http://" />
	</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory>
	<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory android:title="@string/preference_category_display">
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="landscapeWidthMargin"
			android:title="@string/preference_landscapeWidthMargin_title"
			android:summary="@string/preference_landscapeWidthMargin_summary" 
			android:dialogTitle="@string/preference_landscapeWidthMargin_dialogTitle" 
			android:dialogMessage="@string/preference_landscapeWidthMargin_dialogMessage"
			android:defaultValue="0"
			android:inputType="number"/>
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="portraitWidthMargin"
			android:title="@string/preference_portraitWidthMargin_title"
			android:summary="@string/preference_portraitWidthMargin_summary" 
			android:dialogTitle="@string/preference_portraitWidthMargin_dialogTitle" 
			android:dialogMessage="@string/preference_portraitWidthMargin_dialogMessage"
			android:defaultValue="0"
			android:inputType="number"/>
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMListPreference android:key="landscapeColumnNumber"
			android:title="@string/preference_landscapeColumnNumber_title"
			android:summary="@string/preference_landscapeColumnNumber_summary"
			android:dialogTitle="@string/preference_landscapeColumnNumber_dialogTitle"
			android:defaultValue="0"
			android:entries="@array/preference_ColumnNumber_entries"
			android:entryValues="@array/preference_ColumnNumber_entryvalues">
		</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMListPreference>
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMListPreference android:key="portraitColumnNumber"
			android:title="@string/preference_portraitColumnNumber_title" 
			android:summary="@string/preference_portraitColumnNumber_summary"
			android:dialogTitle="@string/preference_portraitColumnNumber_dialogTitle"
			android:defaultValue="0"
			android:entries="@array/preference_ColumnNumber_entries"
			android:entryValues="@array/preference_ColumnNumber_entryvalues">
		</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMListPreference>
	</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory>
	<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory android:title="@string/preference_category_push">
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMCheckBoxPreference android:key="push" 
			android:selectable="true" 
			android:summary="@string/preference_push_activation_summary" 
			android:summaryOff="@string/preference_push_activation_off_summary" 
			android:summaryOn="@string/preference_push_activation_on_summary" 
			android:title="@string/preference_push_activation_title" 
			android:defaultValue="true"/>

		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMRingtonePreference android:key="ringtone" 
			android:title="@string/preference_push_ringtone_title" 
			android:enabled="true" 
			android:ringtoneType="notification|alarm" 
			android:selectable="true" 
			android:showDefault="true" 
			android:showSilent="false" 
			android:summary="@string/preference_push_ringtone_summary" 
			android:defaultValue="content://settings/system/alarm_alert"/>
	</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory>
	<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory android:title="@string/preference_category_proxy">
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="proxylogin"
			android:title="@string/preference_proxylogin_title" 
			android:summary="@string/preference_proxylogin_summary"
			android:dialogTitle="@string/preference_proxylogin_dialogTitle" />
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="proxypassword"
			android:title="@string/preference_proxypassword_title"
			android:summary="@string/preference_proxypassword_summary" 
			android:dialogTitle="@string/preference_proxypassword_dialogTitle"
			android:inputType="textPassword" />
		<com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMEditTextPreference android:key="proxyurlserver"
			android:title="@string/preference_proxyurlserver_title" 
			android:summary="@string/preference_proxyurlserver_summary"
			android:dialogTitle="@string/preference_proxyurlserver_dialogTitle" 
			android:inputType="textUri"/>
	</com.adeuza.movalysfwk.mobile.mf4android.ui.views.preferences.MMPreferenceCategory>
</PreferenceScreen>
