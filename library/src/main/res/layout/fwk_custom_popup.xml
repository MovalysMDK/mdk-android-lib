<?xml version="1.0" encoding="utf-8"?>
<com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMMasterRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/custom_popup_relative_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dialog_text_background"
    android:textColor="@color/dialog_text" >

    <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMSectionTitle
        android:id="@+id/custom_popup__SectionTitle"
        style="?attr/SectionTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMEditText
        android:id="@+id/filtre_section"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/custom_popup__SectionTitle"
        android:layout_marginBottom="5dip"
        android:layout_marginTop="5dip" />


    <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMListView
        android:id="@+id/custom_popup_list_items"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/filtre_section" />

</com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMMasterRelativeLayout>