<?xml version="1.0" encoding="UTF-8"?>
<com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMMasterRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fwksearchactivity__screendetail__master"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMLinearLayout
        android:id="@+id/fwksearchactivity__mainpanel__master"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:orientation="horizontal">
    
        <ScrollView 
            android:id="@+id/fwk_searchactivity_scrollcriteriapanel__master"
    		android:layout_width="wrap_content"
		    android:layout_height="match_parent"
		    android:layout_weight="1">
        
		    <com.adeuza.movalysfwk.mobile.mf4android.ui.views.MMLinearLayout
		        android:id="@+id/fwk_searchactivity_criteriapanel__master"
		        android:layout_width="wrap_content"
